{
  "name": "rain-util-download",
  "version": "0.4.0",
  "author": "Max Millien",
  "description": "Generator based, co/koa compatible file(s) downloader",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/maxmill/rain-util-download.git"
  },
  "license": "MIT",
  "main": "./bin",
  "scripts": {
    "clean": "npm cache clean && rm -rf node_modules bin data",
    "build": "babel lib -d bin",
    "lint":"eslint .",
    "test": "npm run build && tape ./test"
  },
  "keywords": [
    "koa",
    "http",
    "fs",
    "utils",
    "requests",
    "co",
    "download",
    "batch",
    "generators",
    "convenience"
  ],
  "dependencies": {
    "request": "2.x",
    "thunkify-wrap": "1.x",
    "tape-catch": "1.x",
    "co-tape": "*"
  },
  "devDependencies": {
    "babel": "*",
    "babel-cli": "*",
    "babel-preset-es2015-node5": "*",
    "babel-preset-stage-3": "*",
    "eslint": "^3.2.0",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "tape": "4.x"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "babel": {
    "presets": [
      "es2015-node5",
      "stage-3"
    ]
  }
}
